{
	"name": "fab/newsletter-recipients",
	"type": "typo3-cms-extension",
	"description": "Very simplistic database structure to cope with newsletter recipients to be connected with EXT:messenger to send newsletters",
	"homepage": "https://github.com/fabarea/newsletter_recipients",
	"license": [
		"GPL-2.0-or-later"
	],
	"keywords": [
		"TYPO3 CMS",
		"generic listing"
	],
	"support": {
		"issues": "https://github.com/fabarea/newsletter_recipients/issues"
	},
	"autoload": {
		"psr-4": {
			"Fab\\NewsletterRecipients\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "newsletter_recipients"
		}
	},
	"require": {
		"fab/messenger": "^4 || dev-master || dev-develop"
	}
}
